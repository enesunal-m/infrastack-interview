{
  "name": "infrastack-interview",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate": "ts-node scripts/validate.ts",
    "start:user": "cd microservices/user-service && node -r dotenv/config ../../node_modules/.bin/ts-node src/index.ts",
    "start:product": "cd microservices/product-service && node -r dotenv/config ../../node_modules/.bin/ts-node src/index.ts",
    "start:order": "cd microservices/order-service && node -r dotenv/config ../../node_modules/.bin/ts-node src/index.ts",
    "start:all": "concurrently \"npm run start:user\" \"npm run start:product\" \"npm run start:order\"",
    "install:all": "npm install && cd microservices/user-service && npm install && cd ../product-service && npm install && cd ../order-service && npm install"
  },
  "keywords": [
    "infrastack",
    "interview",
    "express",
    "ai",
    "opentelemetry",
    "nextjs"
  ],
  "author": "Muhammed Enes Unal",
  "license": "ISC",
  "description": "A basic monolith project for generating and visualizing mock OpenTelemetry data.",
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "axios": "^0.21.1",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "infrastack-interview-fs-meu-20240829": "^1.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^22.5.1",
    "concurrently": "^6.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.4"
  }
}
